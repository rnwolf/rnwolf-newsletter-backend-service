{
  "dashboard": {
    "id": null,
    "title": "Newsletter Backend Service - Performance & Observability (Staging)",
    "tags": ["newsletter", "cloudflare-workers", "performance", "staging"],
    "uid": "newsletter-backend-staging",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Service Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "newsletter_subscribers_total",
            "legendFormat": "Total Subscribers",
            "refId": "A"
          },
          {
            "expr": "newsletter_subscribers_active",
            "legendFormat": "Active Subscribers",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Newsletter Subscription Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "newsletter_subscriptions_24h",
            "legendFormat": "Subscriptions (24h)",
            "refId": "A"
          },
          {
            "expr": "newsletter_unsubscribes_24h",
            "legendFormat": "Unsubscribes (24h)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Count",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Database Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up",
            "legendFormat": "Service Status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {"text": "Down"},
                  "1": {"text": "Up"}
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Subscriber Growth",
        "type": "graph",
        "targets": [
          {
            "expr": "newsletter_subscribers_total",
            "legendFormat": "Total Subscribers",
            "refId": "A"
          },
          {
            "expr": "newsletter_subscribers_active",
            "legendFormat": "Active Subscribers",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Recent Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "newsletter_subscriptions_24h",
            "legendFormat": "New Subscriptions",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Count",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 6,
        "title": "System Metrics",
        "type": "table",
        "targets": [
          {
            "expr": "newsletter_subscribers_total",
            "legendFormat": "Total Subscribers",
            "refId": "A",
            "instant": true
          },
          {
            "expr": "newsletter_subscribers_active",
            "legendFormat": "Active Subscribers",
            "refId": "B",
            "instant": true
          },
          {
            "expr": "newsletter_subscriptions_24h",
            "legendFormat": "Subscriptions (24h)",
            "refId": "C",
            "instant": true
          },
          {
            "expr": "newsletter_unsubscribes_24h",
            "legendFormat": "Unsubscribes (24h)",
            "refId": "D",
            "instant": true
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "constant",
          "current": {
            "value": "production",
            "text": "production"
          }
        }
      ]
    },
    "annotations": {
      "list": []
    }
  },
  "datasource_config": {
    "name": "Newsletter API Metrics",
    "type": "prometheus",
    "url": "https://api-staging.rnwolf.net/metrics",
    "access": "proxy",
    "basicAuth": false,
    "jsonData": {
      "httpHeaderName1": "Authorization",
      "timeInterval": "30s"
    },
    "secureJsonData": {
      "httpHeaderValue1": "Bearer glsa_YOUR_STAGING_TOKEN_HERE"
    }
  }
}